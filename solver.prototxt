net: "train_val.prototxt"
# lr for fine-tuning should be lower than when starting from scratch
base_lr: 5e-5 #1e-5 
test_iter: 400
test_interval: 500
gamma: 0.1
lr_policy: "multistep"
stepvalue:30000
momentum: 0.9
average_loss: 20
display: 20
max_iter: 8000
weight_decay: 0.0005
snapshot: 500
snapshot_prefix: "/data0/snapshots/dsn_vgg"
solver_mode: GPU